{
  "name": "@htmlacademy/console.js",
  "version": "1.2.8",
  "description": "Inbrowser Demo Console",
  "author": "nikolayemrikh",
  "scripts": {
    "clear": "rm -rf build",
    "copy": "./scripts/copy.sh",
    "test": "npm run test:eslint && npm run test:stylelint && npm run test:karma",
    "test:karma": "gulp test",
    "eslint": "eslint src/",
    "test:stylelint": "stylelint 'sass/**/*.scss' --syntax scss",
    "build": "npm run webpack && npm run copy",
    "webpack": "NODE_ENV=production webpack --progress --no-stats",
    "build:prod": "npm run webpack",
    "serve": "http-server build -p 3501 -o .",
    "start": "npm run copy && npm run webpack && npm run serve",
    "deploy": "npm run build:prod && ./deploy.sh",
    "deploy:exact-version": "npm run build:prod && ONLY_VERSION=true ./deploy.sh"
  },
  "dependencies": {
    "@babel/core": "7.16.12",
    "@babel/plugin-external-helpers": "7.16.7",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.16.11",
    "acorn": "8.7.0",
    "autoprefixer": "10.4.2",
    "babel-plugin-prismjs": "2.1.0",
    "browser-sync": "2.27.7",
    "chai": "4.3.4",
    "core-js": "^3.20.3",
    "del": "6.0.0",
    "eslint": "8.7.0",
    "eslint-config-htmlacademy": "5.0.0",
    "estraverse": "5.3.0",
    "gh-pages": "3.2.3",
    "karma": "6.3.12",
    "karma-babel-preprocessor": "8.0.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-spec-reporter": "0.0.33",
    "lodash.mergewith": "4.6.2",
    "misbehave": "0.3.0",
    "mocha": "9.2.0",
    "postcss-csso": "^6.0.0",
    "prismjs": "1.26.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.12",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "css-mqpacker": "7.0.0",
    "gulp": "4.0",
    "gulp-better-rollup": "3.3.0",
    "gulp-concat": "2.6.1",
    "gulp-csso": "4.0.1",
    "gulp-debug": "4.0.0",
    "gulp-if": "3.0.0",
    "gulp-mocha": "8.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-terser": "2.1.0",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.1",
    "sass": "1.49.0",
    "sass-loader": "13.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "main": "src/console.js",
  "types": "types/console.d.ts",
  "engines": {
    "node": "12.22.5"
